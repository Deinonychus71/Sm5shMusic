<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Styles.Resources>
    <SolidColorBrush x:Key="BrushBorderTitle">#202020</SolidColorBrush>
    <SolidColorBrush x:Key="BrushSmallTitle">#606060</SolidColorBrush>
  </Styles.Resources>

  <Style Selector="ContentControl.list">
    <Setter Property="Background" Value="#E7E7E7"/>
  </Style>
  <Style Selector="Grid.list">
    <Setter Property="Background" Value="#E7E7E7"/>
  </Style>

  <Style Selector="DataGrid.bgmList">
    <Setter Property="AlternatingRowBackground" Value="#F2F2F2"/>
  </Style>

  <Style Selector="Grid.debugConsole">
    <Setter Property="Background" Value="White"/>
  </Style>
</Styles>